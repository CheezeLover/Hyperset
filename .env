# Domain — change this one line to switch domains, then restart gateway
HYPERSET_DOMAIN=hyperset.internal

# Auth crypto key — generate your own with: openssl rand -hex 32
AUTH_CRYPTO_KEY=0f93579f4de2d7910a4da5c8434af800a195498c3705056877deec4db1109858

# OAuth credentials (only needed when using an external provider)
OAUTH_CLIENT_ID=placeholder
OAUTH_CLIENT_SECRET=placeholder
ENTRA_TENANT_ID=common
OIDC_METADATA_URL=https://localhost/.well-known/openid-configuration


# OPENWEBUI config
WEBUI_SECRET_KEY=change-me-to-a-long-random-secret
ENABLE_SIGNUP=true
